@model AcademyBank.Models.Account

@{
    ViewData["Title"] = "Create";
    Layout = "_LayoutAuthenticated";
}

<h1>Create</h1>

<h4>Account</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">

            <div asp-validation-summary="All" class="text-danger"></div>

